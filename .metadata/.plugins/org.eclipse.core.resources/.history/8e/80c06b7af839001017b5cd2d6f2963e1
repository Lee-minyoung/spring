<mapper namespace="com.yedam.web.mapper.empMapper">
	<select id="findAll" resultType="Employees">
		select * from employees
		</select>
		
		<select id="findAlltoMap" resultType="map">
			select employee_id "employee_id",
       			   first_name "first_name",
       			   last_name
       		from employees
		</select>
</mapper>